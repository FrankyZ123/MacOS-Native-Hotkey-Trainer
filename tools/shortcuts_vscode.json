    "shortcuts": [
      {
        "keys": "cmd+x",
        "description": "Cut line (empty selection)",
        "category": "editing",
        "difficulty": 1,
        "tips": ["Cuts entire line when nothing selected", "Great for moving lines quickly"]
      },
      {
        "keys": "cmd+c",
        "description": "Copy line (empty selection)",
        "category": "editing",
        "difficulty": 1,
        "tips": ["Copies entire line when nothing selected", "Faster than selecting the line first"]
      },
      {
        "keys": "alt+up",
        "description": "Move line up",
        "category": "editing",
        "difficulty": 1,
        "tips": ["Moves current line or selection up", "Works with multiple lines selected"]
      },
      {
        "keys": "alt+down",
        "description": "Move line down",
        "category": "editing",
        "difficulty": 1,
        "tips": ["Moves current line or selection down", "Maintains indentation"]
      },
      {
        "keys": "shift+alt+down",
        "description": "Copy line down",
        "category": "editing",
        "difficulty": 1,
        "tips": ["Duplicates current line below", "Works with selections too"]
      },
      {
        "keys": "shift+alt+up",
        "description": "Copy line up",
        "category": "editing",
        "difficulty": 1,
        "tips": ["Duplicates current line above", "Great for repeating patterns"]
      },
      {
        "keys": "cmd+d",
        "description": "Add selection to next find match",
